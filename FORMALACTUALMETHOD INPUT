package selfProgramming;

public class FormalActualMethod {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		showMessage("Ali", 3);
	}
	
	public static void showMessage(String name, int num)
	{
		   for (int i=1; i <= num; i++ )
		   System.out.println("Hello, " + name);
		   
	}

}
