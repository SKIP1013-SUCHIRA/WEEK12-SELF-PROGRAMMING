package selfProgramming;

public class Sales2D {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		double[ ][ ] sales = new double[10][15];

		sales [5][3] = 25.75;

		for(int i=0; i<10; i++)
			for(int j=0; j<15; j++)
				System.out.println("Sales in Row = "+ i + " "+ "Column ="+ j+" is "+ sales[i][j]);

	}

}
